{
    "name": "logfocus",
    "displayName": "LogFocus",
    "description": "Enhanced log analysis with intelligent filtering and focus mode",
    "publisher": "SeanOwO",
    "repository": {
        "type": "git",
        "url": "https://github.com/sean10776/LogFocus.git"
    },
    "icon": "image/new_icon.png",
    "version": "0.3.0",
    "engines": {
        "vscode": "^1.49.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "logfocus.addGroup",
                "title": "Add Group",
                "icon": "$(file-directory-create)"
            },
            {
                "command": "logfocus.editGroup",
                "title": "Edit a new name for This Group",
                "icon": "$(edit)"
            },
            {
                "command": "logfocus.deleteGroup",
                "title": "Delete This Group",
                "icon": "$(dialog-close)"
            },
            {
                "command": "logfocus.toggleHighlight",
                "title": "Toggle Highlight",
                "icon": "$(paintcan)"
            },
            {
                "command": "logfocus.setFocusIncluded",
                "title": "Include in Focus",
                "icon": "$(eye)"
            },
            {
                "command": "logfocus.setFocusExcluded",
                "title": "Exclude from Focus",
                "icon": "$(eye-closed)"
            },
            {
                "command": "logfocus.setFocusNone",
                "title": "Ignore in Focus",
                "icon": "$(circle-slash)"
            },
            {
                "command": "logfocus.turnOnFocusMode",
                "title": "Turn on Focus Mode",
                "icon": "$(symbol-keyword)"
            },
            {
                "command": "logfocus.deleteFilter",
                "title": "Delete This Filter",
                "icon": "$(dialog-close)"
            },
            {
                "command": "logfocus.editFilter",
                "title": "Edit Regex for This Filter",
                "icon": "$(edit)"
            },
            {
                "command": "logfocus.addFilter",
                "title": "Add a Filter",
                "icon": "$(plus)"
            },
            {
                "command": "logfocus.deleteProject",
                "title": "Delete This Project",
                "icon": "$(dialog-close)"
            },
            {
                "command": "logfocus.editProject",
                "title": "Edit a new name for This Project",
                "icon": "$(edit)"
            },
            {
                "command": "logfocus.addProject",
                "title": "Add Project",
                "icon": "$(repo-create)"
            },
            {
                "command": "logfocus.openSettings",
                "title": "Open Settings",
                "icon": "$(gear)"
            },
            {
                "command": "logfocus.refreshSettings",
                "title": "Refresh",
                "icon": "$(refresh)"
            },
            {
                "command": "logfocus.selectProject",
                "title": "Select project"
            },
            {
                "command": "logfocus.exportProject",
                "title": "Export Project",
                "icon": "$(export)"
            },
            {
                "command": "logfocus.importProject",
                "title": "Import Project",
                "icon": "$(symbol-reference)"
            },
            {
                "command": "logfocus.toggleExtension",
                "title": "Toggle LogFocus Extension",
                "icon": "$(eye)"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "command": "logfocus.turnOnFocusMode",
                    "when": "editorTextFocus && !logfocusFocusModeOn",
                    "group": "navigation@4"
                }
            ],
            "view/item/context": [
                {
                    "command": "logfocus.addFilter",
                    "when": "view == filters && viewItem =~ /^g-/",
                    "group": "inline@1"
                },
                {
                    "command": "logfocus.setFocusIncluded",
                    "when": "view == filters && viewItem =~ /(excluded|none)$/",
                    "group": "inline@1.5"
                },
                {
                    "command": "logfocus.setFocusExcluded",
                    "when": "view == filters && viewItem =~ /(included|none)$/",
                    "group": "inline@1.6"
                },
                {
                    "command": "logfocus.setFocusNone",
                    "when": "view == filters && viewItem =~ /(included|excluded)$/",
                    "group": "inline@1.7"
                },
                {
                    "command": "logfocus.editGroup",
                    "when": "view == filters && viewItem =~ /^g-/",
                    "group": "inline@2"
                },
                {
                    "command": "logfocus.editFilter",
                    "when": "view == filters && viewItem =~ /^f-/",
                    "group": "inline@2"
                },
                {
                    "command": "logfocus.editProject",
                    "when": "view == filters.settings && viewItem == project",
                    "group": "inline@2"
                },
                {
                    "command": "logfocus.exportProject",
                    "when": "view == filters.settings && viewItem == project",
                    "group": "inline@3"
                },
                {
                    "command": "logfocus.toggleHighlight",
                    "when": "view == filters",
                    "group": "inline@3"
                },
                {
                    "command": "logfocus.deleteGroup",
                    "when": "view == filters && viewItem =~ /^g-/",
                    "group": "inline@5"
                },
                {
                    "command": "logfocus.deleteFilter",
                    "when": "view == filters && viewItem =~ /^f-/",
                    "group": "inline@5"
                },
                {
                    "command": "logfocus.deleteProject",
                    "when": "view == filters.settings && viewItem == project",
                    "group": "inline@5"
                }
            ],
            "view/title": [
                {
                    "command": "logfocus.addFilter",
                    "when": "view == filters",
                    "group": "navigation@1"
                },
                {
                    "command": "logfocus.addGroup",
                    "when": "view == filters",
                    "group": "navigation@2"
                },
                {
                    "command": "logfocus.addProject",
                    "when": "view == filters.settings",
                    "group": "navigation@1"
                },
                {
                    "command": "logfocus.importProject",
                    "when": "view == filters.settings",
                    "group": "navigation@1.5"
                },
                {
                    "command": "logfocus.openSettings",
                    "when": "view == filters.settings",
                    "group": "navigation@2"
                },
                {
                    "command": "logfocus.refreshSettings",
                    "when": "view == filters.settings",
                    "group": "navigation@3"
                }
            ]
        },
        "keybindings": [
            {
                "command": "logfocus.turnOnFocusMode",
                "key": "ctrl+h",
                "mac": "cmd+h"
            },
            {
                "command": "logfocus.toggleExtension",
                "key": "ctrl+alt+l",
                "mac": "cmd+alt+l"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "filters",
                    "name": "Filters"
                }
            ],
            "filter_project_setting": [
                {
                    "id": "filters.settings",
                    "name": "Projects"
                }
            ]
        },
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "filter_project_setting",
                    "title": "LogFocus",
                    "icon": "image/new_icon.svg"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "lint": "eslint src --ext ts",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "vscode-test"
    },
    "devDependencies": {
        "@types/node": "^24.10.1",
        "@types/vscode": "^1.49.0",
        "@typescript-eslint/eslint-plugin": "^8.47.0",
        "@typescript-eslint/parser": "^8.47.0",
        "@vscode/test-cli": "^0.0.12",
        "@vscode/test-electron": "^2.5.2",
        "eslint": "^9.39.1",
        "typescript": "^5.9.3"
    }
}